<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape>
            <solid android:color="@color/colorWhite" />
            <corners android:radius="@dimen/dp33" />
            <stroke android:width="@dimen/dp1" android:color="@color/colorPrimary" />
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="@color/colorWhite" />
            <corners android:radius="@dimen/dp33" />
            <stroke android:width="@dimen/dp1" android:color="@color/color_border" />
        </shape>
    </item>
</selector>